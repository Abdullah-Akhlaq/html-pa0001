{
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "X-UA-Compatible",
            "value": "IE=edge"
          },
          {
            "key": "Content-Security-Policy",
            "value": "default-src 'self' 'unsafe-inline' 'unsafe-eval'; object-src 'self'"
          },
          {
            "key": "X-Content-Type-Options",
            "value": "nosniff"
          },
          {
            "key": "X-Frame-Options",
            "value": "SAMEORIGIN"
          },
          {
            "key": "Permissions-Policy",
            "value": "accelerometer=(), camera=(), geolocation=(), gyroscope=(), magnetometer=(), microphone=(), payment=(), usb=*"
          }
        ]
      }
    ]
  }